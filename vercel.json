{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/apps/backend/api/index.ts"
        }
    ],
    "buildCommand": "npm run build:contracts && npm run build:backend",
    "functions": {
        "apps/backend/api/index.ts": {
            "includeFiles": "apps/backend/dist/**"
        }
    }
}