{
    "version": 2,
    "builds": {
        "src": "api/index.ts",
        "use": "@vercel/node@3"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ]
        }
    ]
}